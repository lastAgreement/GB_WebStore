@model IEnumerable<WebStore.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <title>Index</title>
</head>
<body>
<table>
    <thead>
        <tr>
            <th>FirstName</th>
            <th>LastName</th>
            <th>Age</th>
            <th>Link</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var emp in Model)
        {
        <tr>
            <td>@emp.FirstName</td>
            <td>@emp.LastName</td>
            <td>@emp.Age</td>
            <td>@Html.ActionLink("Детали", "Details", new { id = emp.Id })</td>
        </tr>
        }
    </tbody>
</table>
</body>
</html>