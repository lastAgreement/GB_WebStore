@model IEnumerable<WebStore.Models.Employee>

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <title>Index</title>
</head>
<body>
<table>
    <thead>
        <tr>
            <th>id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var emp in Model)
        {
        <tr>
            <td>@emp.Id</td>
            <td>@emp.Name</td>
        </tr>
        }
    </tbody>
</table>
</body>
</html>